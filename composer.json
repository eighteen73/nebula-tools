{
    "name": "eighteen73/nebula-tools",
    "description": "A library of tools for Nebula WordPress websites.",
    "authors": [
        {
            "name": "Ed Jeavons",
            "email": "ed@orphans.co.uk"
        },
        {
            "name": "Brett Mason",
            "email": "brett@orphans.co.uk"
        }
    ],
    "type": "library",
    "license": "MIT",
    "autoload": {
        "files": ["includes/hardened-passwords.php"],
        "psr-4": {
            "Eighteen73\\NebulaTools\\": "includes/classes/"
        }
    },
    "minimum-stability": "stable",
    "require": {
        "php": ">=7.4",
        "ext-json": "*"
    },
    "require-dev": {
        "eighteen73/phpcs-composer": "^v1.0"
    },
    "config": {
        "allow-plugins": {
            "dealerdirect/phpcodesniffer-composer-installer": true
        }
    },
    "scripts": {
        "test": [
            "phpcs"
        ]
    }
}
