{
    "name": "eighteen73/nebula-tools",
    "description": "A library of tools for Nebula WordPress websites.",
    "type": "library",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Eighteen73\\NebulaTools\\": "includes/classes/"
        }
    },
    "minimum-stability": "dev",
    "require": {
        "php": ">=7.4"
    },
    "require-dev": {
        "eighteen73/phpcs-composer": "dev-main"
    },
    "config": {
        "allow-plugins": {
            "dealerdirect/phpcodesniffer-composer-installer": true
        }
    }
}
